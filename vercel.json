{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "github": {
    "silent": true
  },
  "rewrites": [
      {
          "source": "/docs/:match*",
          "destination": "https://swirl-docs.vercel.app/docs/:match*"
      },
      {
          "source": "/blog/:match*",
          "destination": "https://swirl-docs.vercel.app/blog/:match*"
      },
      {
          "source": "/assets/:match*",
          "destination": "https://swirl-docs.vercel.app/assets/:match*"
      }
  ]
}